#include <iostream>
#include <cmath>
#include <cstring>
#include <algorithm>
#include <functional>
#include <queue>
#include <vector>
using namespace std;

int arr[1005];

int main()
{
	int n, m;
	scanf("%d%d", &n, &m);

	int x;
	scanf("%d", &x);
	m *= x;

	for (int i = 0; i < n; i++)
		scanf("%d", arr + i);

	sort(arr, arr + n);

	int sum = 0;
	int suc = 0;
	for (int i = n - 1; i >= 0; i--)
	{
		sum += arr[i];
		if (sum >= m)
		{
			printf("%d", n - i);
			suc++;
			break;
		}
	}

	if (!suc)
		printf("STRESS");
}
