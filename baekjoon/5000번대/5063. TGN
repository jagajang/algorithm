#include <iostream>
#include <cmath>
#include <cstring>
#include <algorithm>
#include <functional>
using namespace std;

void print(int done, int notdo)
{
	if (done > notdo)
		printf("advertise\n");
	else if (done == notdo)
		printf("does not matter\n");
	else
		printf("do not advertise\n");
}

int main()
{
	int n;
	scanf("%d", &n);

	for (int i = 0; i < n; i++)
	{
		int a, b, c;
		scanf("%d%d%d", &a, &b, &c);

		print(b - c, a);
	}
}
