#include <iostream>
#include <string>
#include <cstring>
#include <cmath>
#include <algorithm>
#include <functional>
#include <stack>
#include <queue>
#include <vector>
#include <map>
#include <set>
using namespace std;

#define fastio ios_base::sync_with_stdio(false); cin.tie(NULL)

#define uint unsigned int
#define ll long long
#define ul unsigned long long

#define ii pair < int, int >
#define iii pair < int, ii >
#define LL pair < ll, ll >

#define INF 1'000'000'007
#define LINF 1'000'000'000'000'000'007

#define data ll
#define p_q priority_queue < data >
#define np_q priority_queue < data, vector < data >, greater < data > >

double Answer;
vector < pair < ii, ii > > line;
vector < pair < ii, ii > > square[105];

pair < ii, ii > make_four(int a, int b, int c, int d)
{
	return make_pair(make_pair(a, b), make_pair(c, d));
}

int check_valid(pair < ii, ii > a)
{
	if (a.first.first > a.second.first || a.first.second > a.second.second)
		return 0;
	return 1;
}

pair < ii, ii > find_intersect(pair < ii, ii > sq1, pair < ii, ii > sq2)
{
	pair < ii, ii > ret;

	ret.first.first = max(sq1.first.first, sq2.first.first);
	ret.first.second = max(sq1.first.second, sq2.first.second);

	ret.second.first = min(sq1.second.first, sq2.second.first);
	ret.second.second = min(sq1.second.second, sq2.second.second);

	return ret;
}

int main(int argc, char** argv)
{
	fastio;
	int T, test_case;
	cin >> T;

	for (test_case = 0; test_case < T; test_case++)
	{
		Answer = 0;
		line.clear();

		int n;
		cin >> n;

		/*if (n > 30)
		{
			cout << "Case #" << test_case + 1 << endl;
			cout << 0 << endl;
			continue;
		}*/

		int lef = 0, rig = 200'000'000;
		int down = 0, up = 200'000'000;

		line.push_back(make_four(-1, -1, -1, -1));
		for (int i = 1; i <= n; i++)
		{
			int x1, y1, x2, y2;
			cin >> x1 >> y1 >> x2 >> y2;

			if (x1 == x2 && y1 > y2)
				swap(y1, y2);
			else if (x1 > x2)
			{
				swap(x1, x2);
				swap(y1, y2);
			}

			x1 *= 2; y1 *= 2;
			x2 *= 2; y2 *= 2;

			lef = min(lef, min(x1, x2));
			rig = max(rig, max(x1, x2));

			down = min(down, min(y1, y2));
			up = max(up, max(y1, y2));

			line.push_back(make_four(x1, y1, x2, y2));
		}

		int st = 0, en = max(rig - lef, up - down) / 2 + 2;
		while (st <= en)
		{
			for (int i = 0; i <= n; i++)
				square[i].clear();

			int mid = (st + en) / 2;

			ii xy;

			square[0].push_back(make_four(0, 0, 200'000'000, 200'000'000));

			for (int i = 1; i <= n; i++)
			{
				ii xy1 = line[i].first;
				pair < ii, ii > sq1 = make_four(xy1.first - mid, xy1.second - mid, xy1.first + mid, xy1.second + mid);

				for (int j = 0; j < square[i - 1].size(); j++)
				{
					pair < ii, ii > inter = find_intersect(sq1, square[i - 1][j]);

					if (check_valid(inter))
						square[i].push_back(inter);
				}

				ii xy2 = line[i].second;
				pair < ii, ii > sq2 = make_four(xy2.first - mid, xy2.second - mid, xy2.first + mid, xy2.second + mid);

				if (check_valid(find_intersect(sq1, sq2)))
				{
					// intersect exist

					pair < ii, ii > sq3[3] = {
						make_four(sq2.first.first, sq1.second.second, sq1.second.first, sq2.second.second),
						make_four(sq1.second.first, sq2.first.second, sq2.second.first, sq1.second.second),
						make_four(sq1.second.first, sq1.second.second, sq2.second.first, sq2.second.second)
					};

					for (int k = 0; k < 3; k++)
					{
						if (check_valid(sq3[k]) == 0) continue;

						for (int j = 0; j < square[i - 1].size(); j++)
						{
							pair < ii, ii > inter = find_intersect(sq3[k], square[i - 1][j]);

							if (check_valid(inter))
								square[i].push_back(inter);
						}
					}
				}
				else
				{
					// no intersect
					for (int j = 0; j < square[i - 1].size(); j++)
					{
						pair < ii, ii > inter = find_intersect(sq2, square[i - 1][j]);

						if (check_valid(inter))
							square[i].push_back(inter);
					}
				}
			}

			if (square[n].size())
			{
				Answer = 0.5 * mid;
				en = mid - 1;
			}
			else
				st = mid + 1;
		}

		cout << "Case #" << test_case + 1 << endl;
		cout << Answer << endl;
	}

	return 0;
}
